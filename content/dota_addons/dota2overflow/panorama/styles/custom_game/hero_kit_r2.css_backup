
@define team_lay_scale: 30px;
.HeroKit_Root
{
    width: 100%;
    height: 100%;
    vertical-align: top;
    horizontal-align: center;
    margin-top: 0px;
    margin-left: 0px;
    border: 0px solid black;
    /*background-color: #111111;
    background-color: gradient( linear, 0% 0%, 0% 100%, from( #666666 ), to( #222222 ) );*/
	background-image: url("file://{images}/custom_game/overflow_v09.png");
	background-size: 100% 100%;
   /* border-radius: 5px; */
    flow-children: down;
	z-index:5;
}
.ab_button
{
    width: 64px;
    height: 64px;
    border: 2px solid black;
	margin:2px;
	border-radius: 5px;
}
.ab_button:hover
{
    border: 2px solid white;
	border-radius: 5px;
}
.ab_button_selection
{
    width: 80px;
    height: 80px;
    border: 0px solid black;
    horizontal-align: center;
	margin:2px;
	border-radius: 5px;
	box-shadow: #ffffff80 4px 4px 8px 0px;
}

.ab_button_done
{
	width:300px;
	height:55px;
    border-radius: 5px;
    border: 0px solid black;
    horizontal-align: center;
}

.ab_button_selection:hover
{
    border: 4px solid white;
	border-radius: 5px;
}
.ab_button_done:hover
{
	wash-color: #00ff00;
}

#HeroKit_Listing_All
{
    width: 100%;
    height: 600px;
	overflow: clip clip;
    border: 0px solid black;
    flow-children: right;
    horizontal-align: right;
}
#HeroKit_Abilities
{
   /* vertical-align: bottom;
    horizontal-align: left; */
    border: 4px solid black;
    background-color: #333553;
    background-color: gradient( linear, 0% 0%, 0% 100%, from( #22222255 ), to( #22226655 ) );
    border-radius: 5px;
    flow-children: down;
    width: 23%;
    height: 550px;
	padding-top:10px;
	margin-top:15px;
    margin-left:15px;
    margin-right:0px;
	overflow: clip scroll;
	box-shadow: #ffffff80 4px 4px 8px 0px;
}

