<root>
	<styles>
		<include src="file://{resources}/styles/dotastyles.css" />
		<include src="file://{resources}/styles/custom_game/game_info.css" />
	</styles>
	<Panel class="OverflowInfo">
		<Panel id="Title">
			<!--<Label id="TitleName" text="#addonname" />-->
			<Image id="TitleName" class="SeqImg" src="file://{images}/custom_game/loading_screen/overflow_name.png" />
		</Panel>
		<Label id="ObjectiveHeader" class="TextGlowPulse" html="true" text="#info_header_objective" />
		<Label id="BriefDescription" text="#overflow_brief_description" />
		<!--<Label id="HowToPlayHeader" class="Header" html="true" text="#header_how_to_play" />-->
		<!--<Label id="TipsHeader" class="Header" html="true" text="#tips_header" />-->
			<Label id="Tipheader" class="TipText" html="true" text="#tips_tittle" />
		<Panel id="TipsRow" class="InfoRow">
			
			<DOTAAbilityImage class="TipAbIm" id="fire" abilityname="hero_kit_fire_dmg" onmouseover="DOTAShowAbilityTooltip('hero_kit_fire_dmg')" onmouseout="DOTAHideAbilityTooltip()"/>
			<DOTAAbilityImage class="TipAbIm" id="shock" abilityname="hero_kit_shock_dmg" onmouseover="DOTAShowAbilityTooltip('hero_kit_shock_dmg')" onmouseout="DOTAHideAbilityTooltip()"/>
			<DOTAAbilityImage class="TipAbIm" id="water" abilityname="hero_kit_wet" onmouseover="DOTAShowAbilityTooltip('hero_kit_wet')" onmouseout="DOTAHideAbilityTooltip()"/>
			<DOTAAbilityImage class="TipAbIm" id="traps" abilityname="hero_kit_traps" onmouseover="DOTAShowAbilityTooltip('hero_kit_traps')" onmouseout="DOTAHideAbilityTooltip()"/>
			
			<!--
			<Label id="TipText01" class="TipText" html="true" text="#tip01" />
			<Label id="TipText02" class="TipText" html="true" text="#tip02" />
			<Label id="TipText03" class="TipText" html="true" text="#tip03" />
			<Label id="TipText04" class="TipText" html="true" text="#tip04" />-->
		</Panel>
		
	</Panel>
</root>

